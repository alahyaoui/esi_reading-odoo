<odoo>
    <data noupdate="1">

        <!-- Product -->
        <record id="book_product_01" model="product.product">
            <field name="name">Coffret Potter</field>
            <field name="available_in_pos">True</field>
            <field name="standard_price">100.00</field>
            <field name="list_price">80.00</field>
            <field name="weight">3.00</field>
            <field name="to_weight">True</field>
            <field name="type">product</field>
            <field name="categ_id" ref="point_of_sale.product_category_pos"/>
            <field name="uom_id" ref="uom.product_uom_unit"/>
            <field name="uom_po_id" ref="uom.product_uom_unit"/>
            <field name="default_code">BOOK_0001</field>
            <field name="product_books"
                   eval="[(4, ref('book_01'), None),(4, ref('book_02'), None),(4, ref('book_03'), None)]"/>
            <field name="image_1920" type="base64" file="esi_lecture/static/img/harryPotterCollection.jpg"></field>
        </record>

    </data>
</odoo>